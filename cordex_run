mkdir $klasor10_1
cdo splitseas sel$var1$symbol1$date1.nc sel$var1$symbol1$date1
ls *.nc >listobs
for line30 in $(cat listobs)
do
cdo timmean $line30 ./$klasor10_1/$line30
done
#mkdir FORSVM
ls sel$var1_*.nc >list7
arr=(1 5 32 45 50 55 68 95 99)
for line7 in $(cat list7)
do
cdo timmin $line7 min$line7
cdo timmax $line7 max$line7
for item in ${arr[*]}
do
cdo timpctl,$item $line7 min$line7 max$line7 pctl$item$line7
cdo infov pctl$item$line7 >pctl$item$line7.txt
sed 1d pctl$item$line7.txt >apctl$item$line7.txt
awk {'print $10'} apctl$item$line7.txt >a2pctl$item$line7.txt
done
done
rm apctl*.txt
rm pctl*.txt
#rm a2pctl*.txt
arr2=($seas1 $seas2 $seas3 $seas4)
for item in ${arr[*]}
do
paste a2pctl$item*${arr2[0]}.nc.txt a2pctl$item*${arr2[1]}.nc.txt a2pctl$item*${arr2[2]}.nc.txt a2pctl$item*${arr2[3]}.nc.txt >SONUC$item.txt
paste a2pctl5s*${arr2[0]}.nc.txt a2pctl5s*${arr2[1]}.nc.txt a2pctl5s*${arr2[2]}.nc.txt a2pctl5s*${arr2[3]}.nc.txt >SONUC5.txt
done
cat SONUC1.txt SONUC5.txt SONUC95.txt SONUC99.txt >NEGATIF.txt
cat SONUC32.txt SONUC45.txt SONUC50.txt SONUC55.txt SONUC68.txt  >POZTIF.txt
cat SONUC* >HEPSI.txt
mkdir YTIMMEAN
cdo yearmean sel$var1$symbol1$date1.nc ./YTIMMEAN/sel$var1$symbol1$date1.nc
