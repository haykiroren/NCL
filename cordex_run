awk {'print "+1 1:" $1 " 2:" $2" 3:" $3" 4:" $4'} norm_unsucess.txt >train_unsucess.txt
cat train_negatif.txt train_pozitif.txt >TRAIN.txt
cat train_sucess.txt train_unsucess.txt >TEST.txt
$dir3/./svm_learn TRAIN.txt MODEL.txt
