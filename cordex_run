echo "res@cnMinLevelValF = ${arr[0]}" >FILE0_line1
echo "res@cnMaxLevelValF   = ${arr[1]}" >FILE0_line2
echo "res@cnLevelSpacingF = ${arr[2]}" >FILE0_line3
