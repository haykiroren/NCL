pwd
ls limit*.nc >list22
#ls ./RESULT*/FARK/FARK*.nc >list22
mkdir ARA

for line22 in $(cat list22)
do
cdo infov $line22 >./ARA/a$line22.txt
sed 1d ./ARA/a$line22.txt >./ARA/a2$line22.txt
awk {'print $11'} ./ARA/a2$line22.txt > ./ARA/max$line22.txt
awk {'print $9'} ./ARA/a2$line22.txt > ./ARA/min$line22.txt
awk {'print $10'} ./ARA/a2$line22.txt > ./ARA/ort$line22.txt
done

cd ARA
cat max*.txt >$dir8/MAX
cat min*.txt >$dir8/MIN
cat ort*.txt >ORTALAMA$line22.txt
paste ../list22 ORTALAMA$line22.txt >$dir8/RESULTLIMIT$line22-MODELS2.txt
rm a*.txt
rm max*.txt
rm min*.txt
cd ..
rm -rf ARA

cd $dir8
