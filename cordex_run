echo "plot($((i))) = gsn_csm_contour_map_ce(wks,v$((i))(nt,:,:), res)"
((i++)) 
#done < COEF_SUCESS2_list_f2 > ADD_IYI_2_$i
#done < COEF_SUCESS2_list_f2 > ADD_IYI_2
done  < a$line3 > BUYUK2_2ADD$line3
fi
((i++))
done
cp $dir13/ncl_* .
#cp ../FILE0 .
for line3 in $(cat liste3)
do
if [ -e KUCUK1_1$line3 ]
then
cat ncl_1 ADD$line3 FILEKUCUK1_1$line3 ncl_2 FILE0 ncl_3 KUCUK1_1$line3 KUCUK1_1ADD$line3 ncl_4 >$line3.ncl
ncl $line3.ncl
elif [ -e BUYUK2_2$line3 ]
then
if [ -e BUYUK1_1$line3 ]
then
cat ncl_1 ADD$line3 FILEBUYUK1_1$line3 ncl_2 FILE0 ncl_3 BUYUK1_1$line3 BUYUK1_1ADD$line3 ncl_4 >b1_1$line3.ncl
cat ncl_1 ADD$line3 FILEBUYUK2_2$line3 ncl_2 FILE0 ncl_3 BUYUK2_2$line3 BUYUK2_2ADD$line3 ncl_4 >b2_2$line3.ncl
ncl b1_1$line3.ncl
ncl b2_2$line3.ncl
else
echo "Hello"
fi
else
cat ncl_1 ADD$line3 FILEBUYUK2_2$line3 ncl_2 FILE0 ncl_3 BUYUK2_2$line3 BUYUK2_2ADD$line3 ncl_4 >$line3.ncl
ncl $line3.ncl
fi
done
