for line22 in $(cat list22)
do
cdo infov $line22 >./ARA/a$line22.txt
sed 1d ./ARA/a$line22.txt >./ARA/a2$line22.txt
awk {'print $11'} ./ARA/a2$line22.txt > ./ARA/max$line22.txt
awk {'print $9'} ./ARA/a2$line22.txt > ./ARA/min$line22.txt
awk {'print $10'} ./ARA/a2$line22.txt > ./ARA/ort$line22.txt
done
cd ARA
cat max*.txt >MAX
cat min*.txt >MIN
cat ort*.txt >$dir4/RESULT$kla$li.txt
rm a*.txt
rm max*.txt
rm min*.txt
cp $dir4/SONUC_11.R .
Rscript <(cat SONUC_11.R) >../forncl.txt
cd ..
rm -rf ARA
awk {'print $2'} forncl.txt > forncl2.txt
rm forncl.txt
readarray arr < forncl2.txt
echo "res@cnMinLevelValF = ${arr[0]}" >FILE0_line1
echo "res@cnMaxLevelValF   = ${arr[1]}" >FILE0_line2
echo "res@cnLevelSpacingF = ${arr[2]}" >FILE0_line3
cat FILE0_line1 FILE0_line2 FILE0_line3 >FILE0
rm FILE0_*	
#cp $dir15/FILE0 .
echo "f12liUNSUCESS_list.txt f12liUNSUCESS2_list.txt f12COEF_SUCESS2_list.txt f12COEF_SUCESS_list.txt" >liste3
for line3 in $(cat liste3)
do
i=0
IFS=$'\n' read -d '' -r -a lines <$line3
a='"'
vv='"ps"'
vy='"b2_2'
while read line;do
#  if((i==16));then
echo "filename$i = $a${lines[$i]}.nc$a"
echo "f$i = addfile (filename$i, $a r $a)"
echo "v$i = f$i->tas"
echo "v$i@long_name=$a$a"
echo "v$i@units=$a$a"
#  elif((i==18));then
#    echo "filename2 = "${lines[2]}""
#  elif((i==20));then
#    echo "filename3 = "${lines[1]}""
#  elif((i==22));then
#    echo "filename4 = "${lines[4]}""
#  elif((i==24));then
#    echo "filename5 = "${lines[3]}""
#  else
#    echo "$line"
#  fi
((i++))
#done  < COEF_SUCESS2_list_f2 > ADD_IYI
done  < $line3 > ADD$line3
#j=0
echo "baslangıc $i"
if((i<=14));then
echo  "wks = gsn_open_wks($vv,$a$i$il$line3$a)" >FILEKUCUK1_1$line3
echo "plot = new($i,graphic)" >KUCUK1_1$line3
i=0
echo "kucuk onbes $i"
while read line;do
echo "res@tiMainFontHeightF     = 0.023"
echo "res@tiMainString     = $a${lines[$i]}$a"
echo "res@gsnCenterString  = $a $il $date1$a "
echo "plot($i) = gsn_csm_contour_map_ce(wks,v$i(nt,:,:), res)"
((i++))
done  < $line3 > KUCUK1_1ADD$line3
elif((i>=15));then
echo " buyuk onbes $i"
k=$(($i-15))
echo " buyuk onbes k $k"
j=$(($i-$k))
#l= echo $((k+12))
echo "plot = new(15,graphic)" >BUYUK1_1$line3
echo  "wks = gsn_open_wks($vv,$a$i$il$line3$a)" >FILEBUYUK1_1$line3
sed 1,15d $line3 >a$line3
head -15 $line3 >b$line3
while read line;do
echo "res@tiMainFontHeightF     = 0.023"
echo "res@tiMainString     = $a${lines[$((j-15))]}$a"
echo "res@gsnCenterString  = $a $il $date1$a "
echo "plot($((j-15))) = gsn_csm_contour_map_ce(wks,v$((j-15))(nt,:,:), res)"
((k++))
((j++))
done  < b$line3 > BUYUK1_1ADD$line3
echo "plot = new($((k-15)),graphic)" >BUYUK2_2$line3
echo  "wks = gsn_open_wks($vv,$vy$il$line3$a)" >FILEBUYUK2_2$line3
i=$((0))
m= echo $(($i-$k))
while read line;do
echo "res@tiMainFontHeightF     = 0.023"
echo "res@tiMainString     = $a${lines[$i+15]}$a"
echo "res@gsnCenterString  = $a $il $date1$a "
echo "plot($((i))) = gsn_csm_contour_map_ce(wks,v$((i))(nt,:,:), res)"
((i++)) 
#done < COEF_SUCESS2_list_f2 > ADD_IYI_2_$i
#done < COEF_SUCESS2_list_f2 > ADD_IYI_2
done  < a$line3 > BUYUK2_2ADD$line3
