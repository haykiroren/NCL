rev f9liUNSUCESS_list.txt >f10liUNSUCESS_list.txt
cut -c4-46 f10liUNSUCESS_list.txt >f11liUNSUCESS_list.txt
rev f11liUNSUCESS_list.txt >f12liUNSUCESS_list.txt
rev f9liUNSUCESS2_list.txt >f10liUNSUCESS2_list.txt
cut -c4-46 f10liUNSUCESS2_list.txt >f11liUNSUCESS2_list.txt
rev f11liUNSUCESS2_list.txt >f12liUNSUCESS2_list.txt
rev f9COEF_SUCESS_list.txt >f10COEF_SUCESS_list.txt
cut -c4-46 f10COEF_SUCESS_list.txt >f11COEF_SUCESS_list.txt
rev f11COEF_SUCESS_list.txt >f12COEF_SUCESS_list.txt
rev f9COEF_SUCESS2_list.txt >f10COEF_SUCESS2_list.txt
cut -c4-46 f10COEF_SUCESS2_list.txt >f11COEF_SUCESS2_list.txt
rev f11COEF_SUCESS2_list.txt >f12COEF_SUCESS2_list.txt
