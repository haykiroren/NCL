cdo divc,$j j$name1$symbol1$var1$symbol1$date1$seas2.nc $name1$symbol1$var1$symbol1$date1$seas2.nc
cdo divc,$j j$name1$symbol1$var1$symbol1$date1$seas3.nc $name1$symbol1$var1$symbol1$date1$seas3.nc
cdo divc,$j j$name1$symbol1$var1$symbol1$date1$seas4.nc $name1$symbol1$var1$symbol1$date1$seas4.nc
cdo divc,$j j$name1$symbol1$var1$symbol1$date1.nc $name1$symbol1$var1$symbol1$date1.nc
rm j$name1$symbol1$var1$symbol1$date1.nc
rm j$name1$symbol1$var1$symbol1$date1$seas1.nc
rm j$name1$symbol1$var1$symbol1$date1$seas2.nc
rm j$name1$symbol1$var1$symbol1$date1$seas3.nc
rm j$name1$symbol1$var1$symbol1$date1$seas4.nc
