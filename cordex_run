    echo "filename1 = $a$lim.nc$a"
    echo "f1 = addfile (filename1, $a r $a)"
    echo "v1 = f1->tas"
    echo "v1@long_name=$a$a"
    echo "v1@units=$a$a"
done >ADD

echo  "wks = gsn_open_wks($vv,$a$limit$a)" >FILEKUCUK1_1
echo "plot = new(1,graphic)" >KUCUK1_1
integ=1
for line3 in $(cat mevsimm)
do
echo "res@tiMainFontHeightF     = 0.023"
